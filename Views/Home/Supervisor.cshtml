@model IEnumerable<WebApplicationDatabaseProject.supervisors>

@{
    ViewBag.Title = "Supervisor";
}

<h2>Supervisor</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.supervisor_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.supervisor_email)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.supervisor_name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.supervisor_email)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.supervisor_id }) |
            @Html.ActionLink("Details", "Details", new { id=item.supervisor_id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.supervisor_id })
        </td>
    </tr>
}

</table>
