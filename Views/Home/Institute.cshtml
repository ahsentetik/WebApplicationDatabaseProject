@model IEnumerable<WebApplicationDatabaseProject.institute>

@{
    ViewBag.Title = "Institute";
}

<h2>Institute</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.institute_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.university.university_name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.institute_name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.university.university_name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.institute_id }) |
            @Html.ActionLink("Details", "Details", new { id=item.institute_id }) |
