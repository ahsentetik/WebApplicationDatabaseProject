@model IEnumerable<WebApplicationDatabaseProject.thesis>

@{
    ViewBag.Title = "Theses";
}

<h2>Theses</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.title)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.@abstract)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.type)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.year)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.pages)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.submission_date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.author.author_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.institute.institute_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.language.language1)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.supervisors.supervisor_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.topics.topic_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.university.university_name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.title)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.@abstract)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.type)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.year)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.pages)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.submission_date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.author.author_name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.institute.institute_name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.language.language1)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.supervisors.supervisor_name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.topics.topic_name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.university.university_name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.thesis_id }) |
            @Html.ActionLink("Details", "Details", new { id=item.thesis_id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.thesis_id })
        </td>
    </tr>
}

</table>
